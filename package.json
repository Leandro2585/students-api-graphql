{
  "name": "students-api-graphql",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Leandro2585 <leo.real2585@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only -r dotenv/config --inspect=0.0.0.0:9229 src/main/server.ts",
    "lint": "eslint .",
    "lint:fix": "yarn lint -- --fix",
    "prepare": "husky install",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "yarn test --watch",
    "test:staged": "yarn test --findRelatedTests",
    "test:coverage": "yarn test --coverage",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:migration": "yarn typeorm migration:run",
    "typeorm:seed": "ts-node-dev -r tsconfig-paths/register src/infra/typeorm/seeds/index.ts"
  },
  "dependencies": {
    "apollo-server": "^3.4.0",
    "graphql": "^15.6.1",
    "merge-graphql-schemas": "^1.7.8",
    "module-alias": "^2.2.2",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.2.38"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.1",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^27.0.2",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^8.0.29",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^7.0.2",
    "jest": "^27.2.5",
    "jest-mock-extended": "^2.0.4",
    "lint-staged": "^11.2.2",
    "pg-mem": "^2.1.5",
    "ts-jest": "^27.0.5",
    "ts-node": "3.3.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "3.3.3333"
  },
  "engines": {
    "node": "16.x"
  }
}
